@model List<TravelTripProject.Models.Entities.Blog>

<div class="box1-left">

    @foreach (var item in Model)
    {
        <div class="bot-img">
            <img src="@item.ImageUrl" alt="" />
        </div>
        <div class="bot-desc1">
            <p>@(item.Description.Length > 80 ? item.Description.Substring(0, 80) + "..." : item.Description)</p>
        </div>
        <div class="clear"></div>

    }
</div>
